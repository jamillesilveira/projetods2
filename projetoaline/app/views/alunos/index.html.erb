<h2>Alunos</h2>
<table class="alunos">
	<% @aluno.each do |a| %>

	<tr>
		<td><%= a.nome %></td>
		<td><%= link_to image_tag("/exibir.png", :title => "Exibir perfil do aluno"), aluno_path(a) %></td>
		<td><%=link_to image_tag("/editar.png", :title => "Editar perfil do aluno"), edit_aluno_path(a) %></td>
		<td><%=link_to image_tag("/excluir.png", :title => "Excluir perfil do aluno"), a, method: :delete %></td>
	</tr>
	<% end %>
</table>