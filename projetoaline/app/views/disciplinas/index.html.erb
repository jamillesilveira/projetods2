<h2>Disciplinas </h2>

<table border="2">
	<tr>
		<td>id</td>
		<td>Descricao</td>
		<td>Codigo da disciplina</td>
		<td>Id do aluno</td> 
		<td>Criada em</td> 
		<td>Alterada em</td> 
	</tr>
	
	<% @disciplinas.each do |disciplina| %>
	<tr>
		<td><%= disciplina.id  %></td>	
		<td><%= disciplina.codigoDisciplina  %></td>
		<td><%= disciplina.aluno_id  %></td>
		<td><%= disciplina.created_at  %></td>
		<td><%= disciplina.updated_at  %></td>
	</tr>
	<% end %>

</table>
